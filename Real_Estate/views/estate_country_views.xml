<odoo>
    <!-- Form View for Estate Country -->
    <record id="view_estate_country_form" model="ir.ui.view">
        <field name="name">estate.country.form</field>
        <field name="model">estate.country</field>
        <field name="arch" type="xml">
            <form string="Country">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="country_id"/>
                        <field name="region_id"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
